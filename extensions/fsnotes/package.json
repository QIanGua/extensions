{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "fsnotes",
  "title": "FSNotes",
  "description": "Fast interact with fsnotes",
  "icon": "fsnotes-list.png",
  "author": "QIanGua",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search FSNotes",
      "description": "Search and Preview Markdown Files",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}